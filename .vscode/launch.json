{
  "version": "2.0.0",
  "configurations": [
    {
      "name": "Windows Launch",
      "program": "${workspaceFolder}/main.exe",
      "type": "cppvsdbg",
      "request": "launch",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "d:/ProjectDIR/C_Projects/C++/vulkan_tutorial",
      "program": "d:/ProjectDIR/C_Projects/C++/vulkan_tutorial/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}